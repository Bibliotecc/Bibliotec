create database bibliotec; -- Criando a base de dados

use database 

CREATE TABLE `bibliotec`.`livros` (
  `idLivros` INT NOT NULL AUTO_INCREMENT,
  `nomeLivro` VARCHAR(62) NOT NULL,
  `genero` VARCHAR(45) NOT NULL,
  `autor` VARCHAR(100) NOT NULL,
  PRIMARY KEY (`idLivros`));

